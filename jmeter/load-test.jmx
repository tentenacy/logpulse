      <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Generate Log Content">
        <stringProp name="scriptLanguage">beanshell</stringProp>
        <stringProp name="parameters"></stringProp>
        <stringProp name="filename"></stringProp>
        <stringProp name="cacheKey">true</stringProp>
        <stringProp name="script">import java.util.Random;
import java.util.UUID;
import java.text.SimpleDateFormat;

String[] sources = vars.get(&quot;LOG_SOURCES&quot;).split(&quot;,&quot;);
String[] levels = vars.get(&quot;LOG_LEVELS&quot;).split(&quot;,&quot;);

String source = sources[new Random().nextInt(sources.length)];
String level = levels[new Random().nextInt(levels.length)];

String logContent;
String uniqueId = UUID.randomUUID().toString().substring(0, 8);
double errorProb = new Random().nextInt(100);

switch (level) {
    case &quot;ERROR&quot;:
        logContent = &quot;Error processing request: Connection timeout after 30s. Transaction ID: &quot; + uniqueId +
                    &quot;. Stack trace: com.example.ExceptionHandler.processRequest(ExceptionHandler.java:125) &quot; +
                    &quot;at com.example.Controller.handleRequest(Controller.java:89) &quot; +
                    &quot;at com.example.Service.executeRequest(Service.java:211)&quot;;
        break;
    case &quot;WARN&quot;:
        logContent = &quot;Slow database query detected, execution time: 5823ms. Query ID: &quot; + uniqueId +
                    &quot;. Consider optimizing the query: SELECT * FROM users u JOIN orders o ON u.id = o.user_id &quot; +
                    &quot;WHERE o.status = &apos;PROCESSING&apos; AND u.created_at &gt; &apos;2023-01-01&apos;&quot;;
        break;
    case &quot;INFO&quot;:
        logContent = &quot;Successfully processed payment for order ID: ORDER-&quot; + (10000 + new Random().nextInt(90000)) +
                    &quot;. Amount: $&quot; + (50 + new Random().nextInt(950)) + &quot;.99 &quot; +
                    &quot;Payment method: &quot; + (new Random().nextInt(2) == 0 ? &quot;CREDIT_CARD&quot; : &quot;PAYPAL&quot;) +
                    &quot;. Transaction completed in &quot; + (50 + new Random().nextInt(450)) + &quot;ms.&quot;;
        break;
    case &quot;DEBUG&quot;:
        logContent = &quot;Request parameters received: {orderId: &quot; + (10000 + new Random().nextInt(90000)) +
                    &quot;, userId: &quot; + (1000 + new Random().nextInt(9000)) +
                    &quot;, items: &quot; + (1 + new Random().nextInt(10)) +
                    &quot;, totalAmount: &quot; + (50 + new Random().nextInt(950)) +
                    &quot;, shippingMethod: &quot; + (new Random().nextInt(3) == 0 ? &quot;STANDARD&quot; : new Random().nextInt(2) == 0 ? &quot;EXPRESS&quot; : &quot;NEXT_DAY&quot;) +
                    &quot;, couponCode: &quot; + (new Random().nextInt(2) == 0 ? &quot;SUMMER&quot; + new Random().nextInt(100) : &quot;null&quot;) + &quot;}&quot;;
        break;
    default:
        logContent = &quot;Default log message. ID: &quot; + uniqueId;
}

String currentTime = new SimpleDateFormat(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss.SSS&quot;).format(new Date());

vars.put(&quot;LOG_SOURCE&quot;, source);
vars.put(&quot;LOG_LEVEL&quot;, level);
vars.put(&quot;LOG_CONTENT&quot;, logContent);
vars.put(&quot;CURRENT_TIMESTAMP&quot;, currentTime);</stringProp>
      </JSR223PreProcessor>
